type Book{
  id:ID!
  name:String
  author:Author
}

type Author{
  id:ID!
  name:String
}

type Query {
  getAllBooks:[Book]!
  getAllAuthors:[Author]!
}

type Mutation{
  addBook(name:String!,id:ID!):Book
  editBook(id:ID!,name:String!):Book
  deleteBook(id:ID!):Boolean
  addAuthor(name:String!):Author
}